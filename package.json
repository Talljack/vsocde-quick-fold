{
  "publisher": "talljack",
  "name": "vscode-quick-fold",
  "displayName": "vscode-quick-fold",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "description": "Quick Fold/unFold all code",
  "author": "talljack <yugang.cao12@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/talljack",
  "homepage": "https://github.com/talljack/vscode-quick-fold",
  "repository": {
    "type": "git",
    "url": "https://github.com/talljack/vscode-quick-fold"
  },
  "bugs": {
    "url": "https://github.com/talljack/vscode-quick-fold/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/talljack"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.foldAl",
        "title": "Fold All Code"
      },
      {
        "command": "extension.unfoldAl",
        "title": "Unfold All Code"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "nr publish"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.12.12",
    "@types/vscode": "^1.83.0",
    "@vscode/vsce": "^2.26.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "talljack-eslint-config": "^0.2.6",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  }
}
